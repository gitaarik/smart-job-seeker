{
  "name": "portfolio",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "docker:dev": "docker-compose up",
    "docker:dev:init": "dotenvx run -- vite dev --host",
    "docker:dev:cli": "docker compose exec -it app /bin/bash",
    "docker:build-app": "docker-compose exec app npm run build",
    "docker:export-profiles-pdf": "docker-compose exec app npx tsx scripts/export-profiles-pdf.ts",
    "docker:update-schema": "docker-compose exec app npx prisma db pull && docker-compose exec app npx prisma generate",
    "docker:export-profile-json": "bash scripts/export-profile-docker.sh",
    "docker:export-profile-schema": "docker-compose exec app npx tsx scripts/export-profile-schema.ts",
    "docker:export-profile-data": "docker-compose exec app npx tsx scripts/export-profile-data.ts",
    "docker:recover-tech-skills": "docker-compose exec app npx tsx scripts/recover-tech-skills.ts",
    "docker:import-profile-json": "docker-compose exec app npx tsx scripts/import-profile.ts",
    "docker:clear-directus-cache": "docker compose exec app npm run clear-directus-cache",
    "docker:db:cli": "docker-compose exec database psql -U postgres -d smartjobseeker",
    "docker:db:backup": "docker-compose exec database ./scripts/dump-db.sh",
    "docker:db:restore": "docker-compose exec database ./scripts/restore-last-db-dump.sh",
    "docker:db:migrate": "docker-compose exec app npx prisma migrate deploy",
    "docker:db:migrate:new": "docker-compose exec app npx prisma migrate dev --name",
    "docker:db:migrate:deploy": "docker compose exec app npx prisma migrate deploy",
    "docker:db:push:production": "npx tsx scripts/db-push-production.ts",
    "docker:admin:production": "npx dotenvx run -f .env.production -- docker compose -f docker-compose.directus.yml up",
    "build": "npm i && dotenvx run -- prisma generate && dotenvx run -- vite build",
    "rebuild": "npm ci && npm run build",
    "dev-concur": "concurrently --names dev-web,dev-db,dev-directus --prefix-colors red,blue,cyan npm:dev-web npm:dev-db npm:dev-directus",
    "dev-web": "dotenvx run -- vite dev",
    "dev-db": "npx prisma dev",
    "preview": "dotenvx run -- vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "export-profiles-pdf": "npx tsx scripts/export-profiles-pdf.ts",
    "export-profile-schema": "npx tsx scripts/export-profile-schema.ts",
    "export-profile-data": "npx tsx scripts/export-profile-data.ts",
    "recover-tech-skills": "npx tsx scripts/recover-tech-skills.ts",
    "sync-resumes-admin": "cp -r /home/rik/dev/smart-job-seeker/src/lib/exports/* ~/data/Administratie/Freelancing/",
    "build-json-resume": "npx tsx scripts/build-json-resume.ts",
    "render-resume": "resumed render dist/resume.json --output dist/resume.html --theme jsonresume-theme-even",
    "clear-directus-cache": "npx tsx scripts/clear-directus-cache.ts"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-vercel": "^5.8.1",
    "@sveltejs/enhanced-img": "^0.7.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@vitest/ui": "^4.0.8",
    "concurrently": "^9.2.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsonresume-theme-even": "^0.23.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "puppeteer": "^24.16.2",
    "resumed": "^6.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.6",
    "vitest": "^4.0.8",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@directus/sdk": "^20.1.0",
    "@dotenvx/dotenvx": "^1.49.0",
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/svelte-fontawesome": "^0.2.3",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.15.0",
    "@vercel/analytics": "^1.5.0",
    "aos": "^2.3.4",
    "bcryptjs": "^3.0.2",
    "deno": "^2.4.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.19.1",
    "prisma": "^6.15.0",
    "remove-markdown": "^0.6.2",
    "tsx": "^4.20.6",
    "uuid": "^11.1.0"
  }
}
