{
  "name": "portfolio",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "node -r dotenv/config ./node_modules/.bin/vite dev",
    "build": "prisma migrate deploy && prisma generate && prisma generate && node -r dotenv/config ./node_modules/.bin/vite build",
    "preview": "node -r dotenv/config ./node_modules/.bin/vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "export-resume-pdf": "tsx scripts/export-resume-pdf.ts",
    "build-json-resume": "tsx scripts/build-json-resume.ts",
    "render-resume": "resumed render dist/resume.json --output dist/resume.html --theme jsonresume-theme-even"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.5",
    "@eslint/js": "^9.18.0",
    "@sveltejs/adapter-vercel": "^5.8.1",
    "@sveltejs/enhanced-img": "^0.7.0",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^6.1.0",
    "@tailwindcss/vite": "^4.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^3.0.0",
    "globals": "^16.0.0",
    "jsonresume-theme-even": "^0.23.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "puppeteer": "^24.16.2",
    "resumed": "^6.0.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.20.4",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.20.0",
    "vite": "^6.2.6",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/svelte-fontawesome": "^0.2.3",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.15.0",
    "@vercel/analytics": "^1.5.0",
    "aos": "^2.3.4",
    "bcryptjs": "^3.0.2",
    "deno": "^2.4.0",
    "dotenv": "^17.2.1",
    "jsonwebtoken": "^9.0.2",
    "openai": "^5.19.1",
    "prisma": "^6.15.0",
    "uuid": "^11.1.0"
  }
}
