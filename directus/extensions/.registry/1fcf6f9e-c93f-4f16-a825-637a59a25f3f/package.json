{
	"name": "@samfurlong/directus-panel-link",
	"version": "1.0.0",
	"description": "Display a clickable link in a Directus panel",
	"main": "dist/index.js",
	"license": "MIT",
	"icon": "extension",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-extension-panel"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"directus:extension": {
		"type": "panel",
		"path": "dist/index.js",
		"source": "src/index.ts",
		"host": "^11.5.1"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"validate": "directus-extension validate"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "13.1.1",
		"typescript": "^5.8.3",
		"vue": "^3.5.16"
	}
}
